package com.imin.domain;

/**
 * class：书籍实体类
 * author：patmiu
 * time：2018年9月29日下午4:57:58
 */
public class Book {

	/**
	 * 书籍的id
	 */
	private String bid;
	/**
	 * 书籍的名称
	 */
	private String name;
	/**
	 * 书籍的价格
	 */
	private Double price;
	/**
	 * 书籍的折扣价
	 */
	private Double discountprice;
	/**
	 * 书籍的小图片
	 */
	private String imgurl;
	/**
	 * 书籍的详细描述
	 */
	private String detail;
	/**
	 * 书籍的折扣
	 */
	private Double discount;
	/**
	 * 书籍的审核状态（1 审核通过    0：未通过 在后台审核，未通过审核的不能再前台显示和购买）
	 */
	private Integer state;
	/**
	 * 购买书籍的数量(购物车或购买的时候用到)
	 */
	private Integer buyNum;
	/**
	 * 书籍的类型
	 */
	private String type;
	/**
	 * 书籍的期限
	 */
	private String limittime;
	/**
	 * 书籍适合的专业
	 */
	private Integer specialty;
	/**
	 * 书籍适合的年级
	 */
	private Integer grade;
	/**
	 * 书籍的作者
	 */
	private String author;
	/**
	 * 书籍的出版社
	 */
	private String press;
	/**
	 * 书籍的出版时间
	 */
	private String pubdate;
	/**
	 * 书籍的国际标准标号，简称ISBN
	 */
	private String isbn;
	/**
	 * 买这本书能够得到的积分
	 */
	private Integer credit;
	/**
	 * 书籍的供应商名称
	 */
	private String sellername;
	/**
	 * 上传书籍的时间
	 */
	private String uploadbooktime;
	/**
	 * 放大后的图片的路径
	 */
	private String homeimageurl;
	/**
	 * 书籍的库存数
	 */
	private Integer stock;
	/**
	 * 书籍是否能够用积分兑换 1:可以，0:不可以
	 */
	private Integer exchagable;
	/**
	 * 书籍已出售的总数
	 */
	private Integer sold;
	/**
	 * 书籍的收藏数
	 */
	private Integer collection;
	
	public String getBid() {
		return bid;
	}
	public void setBid(String bid) {
		this.bid = bid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Double getPrice() {
		return price;
	}
	public void setPrice(Double price) {
		this.price = price;
	}
	public Double getDiscountprice() {
		return discountprice;
	}
	public Integer getBuyNum() {
		return buyNum;
	}
	public void setBuyNum(Integer buyNum) {
		this.buyNum = buyNum;
	}
	public void setDiscountprice(Double discountprice) {
		this.discountprice = discountprice;
	}
	public String getImgurl() {
		return imgurl;
	}
	public void setImgurl(String imgurl) {
		this.imgurl = imgurl;
	}
	public String getDetail() {
		return detail;
	}
	public void setDetail(String detail) {
		this.detail = detail;
	}
	public Double getDiscount() {
		return discount;
	}
	public void setDiscount(Double discount) {
		this.discount = discount;
	}
	public Integer getState() {
		return state;
	}
	public void setState(Integer state) {
		this.state = state;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getLimittime() {
		return limittime;
	}
	public void setLimittime(String limittime) {
		this.limittime = limittime;
	}
	public Integer getSpecialty() {
		return specialty;
	}
	public void setSpecialty(Integer specialty) {
		this.specialty = specialty;
	}
	public Integer getGrade() {
		return grade;
	}
	public void setGrade(Integer grade) {
		this.grade = grade;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	public String getPress() {
		return press;
	}
	public void setPress(String press) {
		this.press = press;
	}
	public String getPubdate() {
		return pubdate;
	}
	public void setPubdate(String pubdate) {
		this.pubdate = pubdate;
	}
	public String getIsbn() {
		return isbn;
	}
	public void setIsbn(String isbn) {
		this.isbn = isbn;
	}
	public Integer getCredit() {
		return credit;
	}
	public void setCredit(Integer credit) {
		this.credit = credit;
	}
	public String getSellername() {
		return sellername;
	}
	public void setSellername(String sellername) {
		this.sellername = sellername;
	}
	public String getUploadbooktime() {
		return uploadbooktime;
	}
	public void setUploadbooktime(String uploadbooktime) {
		this.uploadbooktime = uploadbooktime;
	}
	public String getHomeimageurl() {
		return homeimageurl;
	}
	public void setHomeimageurl(String homeimageurl) {
		this.homeimageurl = homeimageurl;
	}
	public Integer getStock() {
		return stock;
	}
	public void setStock(Integer stock) {
		this.stock = stock;
	}
	public Integer getExchagable() {
		return exchagable;
	}
	public void setExchagable(Integer exchagable) {
		this.exchagable = exchagable;
	}
	public Integer getSold() {
		return sold;
	}
	public void setSold(Integer sold) {
		this.sold = sold;
	}
	public Integer getCollection() {
		return collection;
	}
	public void setCollection(Integer collection) {
		this.collection = collection;
	}
	

}
